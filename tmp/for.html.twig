<ul>
  {% for user in users %}
    <li>{{ user.name }}</li>
  {% endfor %}
</ul>

<ul>
  {% for key, user in users %}
    {% set name = user.name %}
    <li>{{ name }}</li>
  {% endfor %}
</ul>

<ul>
  {% for key, user in users|slice(0, 10) if user.name != 'foo' and user.id != 'bar' %}
    <li>{{ user.name }}</li>
  {% endfor %}
</ul>
